_me_completion() {
  local cur prev words cword
  _get_comp_words_by_ref cur prev words cword

  ME_SUBCMDS=(
    $(find M4_LIB_ME_DIR -name 'me-*' -printf "%f\n" | cut -c 4-) )

  (( $cword == 1 )) && {
    COMPREPLY=( $(compgen -W "${ME_SUBCMDS[*]}" $cur) )
    return
  }
}

complete -F _me_completion me
